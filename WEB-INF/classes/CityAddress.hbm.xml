<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
      <class name="model.CityAddress"  table="allcityaddress">
          <id name="id" column="id" type="int">
          <generator class="increment"></generator>
          </id>
          
      <property name="city_real" column="city_real" type="string"></property>
      <property name="lng" column="lng" type="float"></property>
      <property name="lat" column="lat" type="float"></property>
      </class>
</hibernate-mapping>