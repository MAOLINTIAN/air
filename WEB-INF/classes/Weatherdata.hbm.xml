<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
      <class name="model.Weatherdata"  table="weatherdata">
          <id name="id" column="id" type="int">
          <generator class="increment"></generator>
          </id>
       <property name="city" column="city" type="string"></property>   
      <property name="city_real" column="city_real" type="string"></property>
      <property name="datetime" column="datetime" type="timestamp"></property>
      <property name="province" column="province" type="string"></property>
      <property name="htemper" column="htemper" type="string"></property>
      <property name="Itemper" column="Itemper" type="string"></property>
      <property name="status" column="status" type="string"></property>
      <property name="wind" column="wind" type="string"></property>
  
      </class>
</hibernate-mapping>